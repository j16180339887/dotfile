[core]
    editor = vim -c 'normal! gg'
    autocrlf = false
    safecrlf = false
    pager = vim +'setlocal gdefault' +'%sm/\\e.\\{-}m//e' +'call histdel(\"search\", -1)' +'setlocal ft=diff' +'setlocal buftype=nofile' +'normal! gg' -
    fileMode = true
[rerere]
    enabled = 1
    autoupdate = 1
[color]
    ui = auto
[diff]
    tool = vimdiff
[difftool]
    prompt = false
[http]
    sslVerify = false
[commit]
    template = ~/.gitmessage
