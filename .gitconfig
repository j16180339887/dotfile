[core]
    editor = vim -c 'normal! gg'
    autocrlf = false
    safecrlf = false
    fileMode = true
    quotepath = false
    pager = vim +'setlocal gdefault' +'%sm/\\e.\\{-}m//e' +'g/^+++/d' +'g/^---/d' +'call histdel(\"search\", -1)' +'setlocal ft=diff' +'setlocal buftype=nofile' +'normal! gg' +'redir => aaa | silent exe \"%s/^+//ne\" | redir END | redir => ddd | silent exe \"%s/^-//ne\" | redir END | noh | redraw | echo \" Git message: \" . matchstr( aaa, \"\\\\d\\\\+\" ) . \" additions and \" . matchstr( ddd, \"\\\\d\\\\+\" ) . \" deletions\"' -
[rerere]
    enabled = 1
    autoupdate = 1
[color]
    ui = auto
[diff]
    tool = vimdiff3
[difftool]
    prompt = false
[http]
    sslVerify = false
[commit]
    template = ~/.gitmessage
[merge]
    conflictstyle = diff3
    tool = vimdiff3
[gui]
    encoding = utf-8
[mergetool]
    prompt = false
[pull]
    rebase = true
