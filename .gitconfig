[user]
	name = Joeky Zhan
	email = jj16180339887@gmail.com
[rerere]
	enabled = 1
	autoupdate = 1
[color]
	ui = auto
[diff]
	tool = vimdiff3
[difftool]
	prompt = false
[http]
	sslVerify = false
[commit]
	template = ~/.gitmessage
[merge]
	conflictstyle = diff3
	tool = vimdiff3
[gui]
	encoding = utf-8
[mergetool]
	prompt = false
[pull]
	rebase = true
[alias]
	history = log -p
	summary = log --stat
[core]
	editor = vim -c 'normal! gg'
	autocrlf = false
	safecrlf = false
	fileMode = true
	quotepath = false
	pager = vim +' \
setlocal gdefault |\
%sm/\\e.\\{-}m//e |\
setlocal ft=git |\
setlocal buftype=nofile |\
exe \"g/^+++/d\" |\
exe \"g/^---/d\" |\
call histdel(\"search\", -1) |\
exe \"normal! gg\" |\
hi iBlueInfo ctermfg=0 ctermbg=39 |\
echo \"aaa\" |\
redir => aaa |\
silent exe \"%s/^+//ne\" |\
redir END |\
redir => ddd |\
silent exe \"%s/^-//ne\" |\
redir END |\
noh |\
redraw! |\
let aaa = matchstr( aaa, \"\\\\d\\\\+\" ) |\
let ddd = matchstr( ddd, \"\\\\d\\\\+\" ) |\
let aaa = aaa == \"\" ? \"+0\" : \"+\".aaa |\
let ddd = ddd == \"\" ? \"-0\" : \"-\".ddd |\
exe \"set ul=-1 | m-1 | let &ul=\" . &ul |\
echohl iBlueInfo | echon \"		Info	 \" |\
echohl Function    | echon \" \" . aaa . \" additions\" |\
echohl Conditional | echon \" \" . ddd . \" deletions\" |\
echohl None ' -
