set expandtab
set tabstop=4
set shiftwidth=4
set nobackup
set nowritebackup
set noswapfile
nnoremap <S-Tab> <<
inoremap <S-Tab> <C-d>
set nocompatible
set backspace=2
imap <C-BS> <C-W>
map  <C-Del> dw
imap <C-Del> <ESC>dwi
" set number
" " set smartindent
" " set ai
" " syntax on
" " imap { {}<ESC>i
" " imap [ []<ESC>i
" " imap ( ()<ESC>i
